{
  "version": 1,
  "author": "ESP32-S2 MQTT DHT22 LED",
  "editor": "wokwi",
  "parts": [
    { "type": "board-esp32-s2-devkitm-1", "id": "esp", "top": 0, "left": 0, "attrs": {} },
    { "type": "wokwi-resistor", "id": "r1", "top": 30, "left": -60, "rotate": 90, "attrs": {} },
    { "type": "wokwi-led", "id": "led1", "top": -50, "left": -58, "attrs": { "color": "red" } }
  ],
  "connections": [
    [ "esp:TX", "$serialMonitor:RX", "", [] ],
    [ "esp:RX", "$serialMonitor:TX", "", [] ],

    [ "esp:15", "r1:2", "green", [ "h0" ] ],
    [ "r1:1", "led1:A", "green", [ "h0" ] ],
    [ "esp:GND.1", "led1:C", "black", [ "h0" ] ]
  ]
}